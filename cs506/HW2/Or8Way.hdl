/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    // Put your code here:
    Or4Way (in=in[0..3], out=in0123);
    Or4Way (in=in[4..7], out=in4567);
    Or (a=in0123, b=in4567, out=out);
}
