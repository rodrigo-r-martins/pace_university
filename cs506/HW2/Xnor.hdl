/**
 * Exclusive-or gate:
 * out = (a == b)
 */


CHIP Xnor {
    IN a, b;
    OUT out;

    PARTS:
    	
    // Put your code here:
    Xor (a=a, b=b, out=axorb);
    Not (in=axorb, out=out);
}
